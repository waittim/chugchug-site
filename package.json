{
  "name": "chugchug-site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run prerender",
    "prerender": "react-snap",
    "preview": "vite preview"
  },
  "reactSnap": {
    "source": "dist",
    "include": [
      "/",
      "/privacy.html",
      "/zh/",
      "/zh/privacy.html"
    ],
    "skipThirdPartyRequests": true
  },
  "dependencies": {
    "lucide-react": "^0.542.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "vite": "^5.4.2",
    "react-snap": "^1.23.0"
  }
}
